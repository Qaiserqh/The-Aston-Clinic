sel>\n            {treatments.imageurls.map(url=>{\n              return <Carousel.Item>\n              <img\n                className=\"d-block w-100 bigimg\"\n                src={url}\n              />\n            </Carousel.Item>\n            })}\n          </Carousel>\n          <p>{treatments.description}</p>\n        </Modal.Body>\n        <Modal.Footer>\n          <Button onClick={handleClose}>\n            Close\n          </Button>\n        </Modal.Footer>\n      </Modal>\n\n    </div>\n  )\n}\n\nexport default Publish","/Users/qaiser/Desktop/the-aston-clinic/src/screens/BookingScreen.js",["91","92","93"],[],"/Users/qaiser/Desktop/the-aston-clinic/src/components/Loader/Loader.js",["94","95","96"],[],"import React from 'react'\nimport MoonLoader from \"react-spinners/MoonLoader\";\nimport { useState } from \"react\";\nimport '../../Utilities/Loader.css'\n\nfunction Loader() {\n\n  let [loading, setLoading] = useState(true);\n  let [color, setColor] = useState(\"#ffffff\");\n\n\n\n  return (\n    <div className='text-center mtop'>\n    <div className=\"sweet-loading flex pos-center\">\n      <MoonLoader\n        color='#C9AE5D'\n        loading={loading}\n        css=''\n        size={80}\n      />\n    </div>\n    </div>\n  )\n}\n\nexport default Loader","/Users/qaiser/Desktop/the-aston-clinic/src/components/Error/Error.js",[],[],"/Users/qaiser/Desktop/the-aston-clinic/src/screens/Login.js",["97"],[],"import React, {useState, useEffect} from 'react'\nimport { Button }  from '../components/Button/Button'\nimport '../Utilities/Login-Register.css'\nimport Loader from '../components/Loader/Loader'\nimport Error from '../components/Error/Error'\nimport axios from 'axios'\n\nfunction Login() {\n\n    const[email, setemail] = useState('')\n    const[password, setpassword] = useState('')\n    const [loading, setloading] = useState();\n    const [error, seterror] = useState();\n\n    async function login(){\n            const user={\n                email,\n      